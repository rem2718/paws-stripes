    <%- include('./partials/head')%>
  <style>
    .card {
      width: 200px;
      height: 300px;
      background-color: #f2f2f2;
      border: 1px solid #ddd;
      box-shadow: 0px 0px 5px 0px rgba(0, 0, 0, 0.2);
      margin: 10px;
    }

    .grid {
      display: grid;
      grid-template-columns: repeat(3, 1fr);
      grid-gap: 20px;
    }

    .card img {
      width: 100%;
      height: 70%;
      object-fit: cover;
    }

    .card-text {
      padding: 10px;
    }
  
  </style>
</head>
